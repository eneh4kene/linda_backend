# Environment Variables for Linda Backend
# Copy these to Railway's Variables tab (or create a .env file for local development)

# Database
DATABASE_URL=postgresql://user:password@host:5432/database

# Redis
REDIS_URL=redis://host:6379

# Retell AI
RETELL_API_KEY=your_retell_api_key
RETELL_AGENT_ID=your_retell_agent_id
RETELL_PHONE_NUMBER=+1234567890
RETELL_WEBHOOK_SECRET=your_webhook_secret
RETELL_TEST_MODE=false

# Anthropic (Claude)
ANTHROPIC_API_KEY=your_anthropic_api_key

# Vercel Blob Storage
BLOB_READ_WRITE_TOKEN=your_blob_token

# Server Configuration
PORT=3000
NODE_ENV=production
API_BASE_URL=https://your-app.railway.app

# Authentication
JWT_SECRET=your_jwt_secret_minimum_32_characters_long
JWT_EXPIRES_IN=7d

# Frontend (Optional)
FRONTEND_URL=https://your-frontend-url.com

